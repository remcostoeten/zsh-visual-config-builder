{
	"name": "zsh-visual-config-builder",
	"version": "0.1.0",
	"private": true,
	"author": {
		"name": "Remco Stoeten",
		"url": "https://zsh-config-builder.remcostoeten.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/remcostoeten/zsh-visual-config-builder"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "biome check .",
		"f": "biome format . --write",
		"fix:ui": "python3 scripts/fix-imports/optimize-ui-imports.py --all && read -p \"UI is fixed. Do you want to run pnpm f (biome formatter)? y/N: \" -n 1 -r; echo    # (optional) move to a new line\nif [[ $REPLY =~ ^[Yy]$ ]]; then\n  pnpm f\nfi",
		"fix:ui-file": "python3 scripts/fix-imports/optimize-ui-imports.py --file",
		"createdb": "python3 scripts/create-db.py",
		"gen": "drizzle-kit generate",
		"push": "drizzle-kit push",
		"clear": "rm -rf node_modules && rm -rf package-lock.json && rm -rf yarn.lock && rm -rf package-lock.json && rm -rf yarn.lock",
		"rebuild": "pnpm clear && pnpm i && pnpm build",
		"ui": "pnpm dlx shadcn add"
	},
	"dependencies": {
		"@floating-ui/react-dom-interactions": "^0.13.3",
		"@libsql/client": "^0.14.0",
		"@radix-ui/react-avatar": "^1.1.2",
		"@radix-ui/react-collapsible": "^1.1.2",
		"@radix-ui/react-dropdown-menu": "^2.1.4",
		"@t3-oss/env-nextjs": "^0.11.1",
		"@tanstack/react-query": "^5.62.11",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.38.3",
		"framer-motion": "^11.15.0",
		"jose": "^5.9.6",
		"lucide-react": "^0.469.0",
		"nanoid": "^5.0.9",
		"next": "15.1.0",
		"next-themes": "^0.4.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"sonner": "^1.7.1",
		"tailwind-merge": "^2.6.0",
		"tailwindcss-animate": "^1.0.7",
		"ua-parser-js": "^2.0.0",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^20",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"drizzle-kit": "^0.30.1",
		"postcss": "^8",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	}
}
